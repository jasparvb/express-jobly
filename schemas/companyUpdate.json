{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "name": "Walmart Inc.",
            "num_employees": 2000,
            "description": "A family friendly superstore",
            "logo_url": "https://cdn.corporate.walmart.com/logo.jpg",
            "_token": "2039ur09w0r909s09094334"
        }
    ],
    "required": [
        "name"
    ],
    "additionalProperties": false,
    "properties": {
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Walmart Inc."
            ]
        },
        "num_employees": {
            "$id": "#/properties/num_employees",
            "type": "integer",
            "title": "The num_employees schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                2000
            ]
        },
        "description": {
            "$id": "#/properties/description",
            "type": "string",
            "title": "The description schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "A family friendly superstore"
            ]
        },
        "logo_url": {
            "$id": "#/properties/logo_url",
            "type": "string",
            "title": "The logo_url schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "format": "uri",
            "examples": [
                "https://cdn.corporate.walmart.com/logo.jpg"
            ]
        },
        "_token": {
            "$id": "#/properties/token",
            "type": "string",
            "title": "The Token Schema ",
            "default": "",
            "examples": [
              "2l34kl2hl445346jklkjsdlsk"
            ]
        }
    }
}